package operator;

public class Logical2 {
    public static void main(String[] args) {
        int a = 15;
        //a는 10보다 크고 20보다 작다
        boolean result = a > 10 && a < 20; //연산 순서 (a < 10) && (a < 20) 괄호 먼저
        //boolean result = 10 <a && a < 20; 조금 더 읽기 편하다 번위를 나타내기 때문에
        System.out.println("result = " + result);

    }
}
