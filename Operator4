package operator;

public class Operator4 {
    public static void main(String[] args) {
        int sum3 = 2 * 2 + 3 * 3;
        int sum4 = (2 * 2) + (3 * 3);//sum3과 같다
        System.out.println("sum3 = " + sum3);
        System.out.println("sum4 = " + sum4);

        //연산이 복잡하거나 연산자 우선순위가 애매한 경우에는 sum4처럼 괄호를 넣는게 명확하다.
    }
}
