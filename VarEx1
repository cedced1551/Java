package valiable.ex;

// 다음 코드에 반복해서 나오는 숫자 4,3을 다른 숫자로 한번에 변경할 수 있도록 다음을 변수 num1, num2를 사용해 변경하여라.
public class VarEx1Question {
    public static void main(String[] args) {
        int num1 = 4;
        int num2 = 3;

        System.out.println(num1 + num2);
        System.out.println(num1 - num2);
        System.out.println(num1 * num2);
    }
}
